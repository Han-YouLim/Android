<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.studentmanagement">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.StudentManagement">
        <activity android:name=".PersonalReg"
            android:label="학생 관리 시스템"
            android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".PersonalList"
            android:label="학생 관리 시스템2"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".PersonalInfo"
            android:label="학생 관리 시스템3"/>
        <activity android:name=".MainActivity"
            android:label="학생 관리 시스템4">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>